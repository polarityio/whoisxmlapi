<div class="link-container">
  <a class="p-link" href="https://user.whoisxmlapi.com/products" target="_blank">
    View EndPoint Balance on
    <span class="whois-link-name">Whois<span class="title-xml">XML</span><span
        class="title-api"
      >API</span></span>
    {{fa-icon icon="external-link-square" class="external-link-icon"}}
  </a>
</div>

<ul class="nav nav-tabs">
  {{#if (or (eq block.entity.type "string")(eq block.entity.type "email"))}}
    {{#if (get details "reverseWhois" "length")}}
      <li class="nav-item">
        <a
          {{action "changeTab" "reverseWhois"}}
          class="nav-link {{if (eq block._state.activeTab 'reverseWhois') 'active'}}"
          href="#"
        >
          Reverse WHOIS
        </a>
      </li>
    {{/if}}
  {{else}}
    {{#if (get details "whois" "length")}}
      <li class="nav-item">
        <a
          {{action "changeTab" "whois"}}
          class="nav-link {{if (eq block._state.activeTab 'whois') 'active'}}"
          href="#"
        >
          WHOIS
        </a>
      </li>
    {{/if}}
    {{#if block.entity.isDomain}}
      <li class="nav-item">
        <a
          {{action "changeTab" "dns"}}
          class="nav-link {{if (eq block._state.activeTab 'dns') 'active'}}"
          href="#"
        >
          DNS Lookup
          {{#if getDnsIsRunning}}
            {{fa-icon icon="spinner-third" size="md" fixedWidth=true spin=true}}
          {{/if}}
        </a>
      </li>

      <li class="nav-item">
        <a
          {{action "changeTab" "dnsWhoisHistory"}}
          class="nav-link {{if (eq block._state.activeTab 'dnsWhoisHistory') 'active'}}"
          href="#"
        >
          WHOIS History
          {{#if getDnsWhoisHistoryIsRunning}}
            {{fa-icon icon="spinner-third" size="md" fixedWidth=true spin=true}}
          {{/if}}
        </a>
      </li>

      <li class="nav-item">
        <a
          {{action "changeTab" "domainAvailability"}}
          class="nav-link {{if (eq block._state.activeTab 'domainAvailability') 'active'}}"
          href="#"
        >
          Domain Availability
          {{#if getDomainAvailabilityIsRunning}}
            {{fa-icon icon="spinner-third" size="md" fixedWidth=true spin=true}}
          {{/if}}
        </a>
      </li>

      <li class="nav-item">
        <a
          {{action "changeTab" "domainSubDomain"}}
          class="nav-link {{if (eq block._state.activeTab 'domainSubDomain') 'active'}}"
          href="#"
        >
          Subdomain Discovery
          {{#if getDomainSubDomainIsRunning}}
            {{fa-icon icon="spinner-third" size="md" fixedWidth=true spin=true}}
          {{/if}}
        </a>
      </li>
    {{/if}}
    <li class="nav-item">
      <a
        {{action "changeTab" "reverseNs"}}
        class="nav-link {{if (eq block._state.activeTab 'reverseNs') 'active'}}"
        href="#"
      >
        Reverse NS
        {{#if getReverseNsIsRunning}}
          {{fa-icon icon="spinner-third" size="md" fixedWidth=true spin=true}}
        {{/if}}
      </a>
    </li>
  {{/if}}
</ul>
<br />

