{{#if (eq block._state.activeTab "reverseWhois")}}
  <div class="reverse-whois">
    {{#each displayReverseWhois as |reverseWhois reverseWhoisIndex|}}
      <div class="reverse-whois-item">
        <button
          class="copy-btn"
          title="Copy to Clipboard"
          {{action "copyData" reverseWhoisIndex}}
        >
          <div id={{reverseWhoisIndex}}>
            {{fa-icon icon="copy" class="icon" fixedWidth=true}}
            {{reverseWhois}}
          </div>
        </button>
        {{#if (get showCopySuccessCheck reverseWhoisIndex)}}
          {{fa-icon icon="check" class="icon" fixedWidth=true}}
        {{/if}}
      </div>
    {{/each}}
    {{#if (lt displayReverseWhois.length details.reverseWhois.length)}}
      <div class="load-more"><a
          href="#"
          {{on
            "click"
            (action "loadMoreDisplayItems" "displayReverseWhois" "details.reverseWhois")
          }}
        >
          ... Load More
        </a></div>
    {{/if}}
  </div>
{{/if}}

{{! Expand On Message Tabs }}
{{#if (eq block._state.activeTab "dns")}}
  <div class="dns">
    {{#if getDnsIsRunning}}
      {{fa-icon icon="spinner-third" size="lg" fixedWidth=true spin=true}}
    {{else if gettingDnsErrorMessage}}
      <div class="alert alert-danger mt-1">
        {{gettingDnsErrorMessage}}
      </div>
    {{else if dnsFailureMessage}}
      <div class="failure-response">
        {{dnsFailureMessage}}
      </div>
    {{else if block._state.dnsHasBeenObtained}}
      {{#if (not dns.dnsRecords.length)}}
        <span class="no-results">No Results Found</span>
      {{else}}
        <div class="dns-content">
          {{#if dns.domainName}}
            <h1 class="p-title">
              {{fa-icon icon="location-arrow" class="icon" fixedWidth=true}}
              {{dns.domainName}}
            </h1>
          {{/if}}

          {{#if dns.audit.createdDate}}
            <div>
              <span class="p-key">Audit Created Date: </span>
              <span class="p-value">{{dns.audit.createdDate}}</span>
            </div>
          {{/if}}
          {{#if dns.audit.updatedDate}}
            <div>
              <span class="p-key">Audit Updated Date: </span>
              <span class="p-value">{{dns.audit.updatedDate}}</span>
            </div>
          {{/if}}

          {{#if (get expandableTitleStates (concat "dns" "records"))}}
            <a
              href="#"
              class="expandable-title"
              {{on "click" (action "toggleExpandableTitle" "dns" "records")}}
            >
              <span class="text">{{fa-icon icon="signature" class="icon" fixedWidth=true}}
                Records</span>
              {{fa-icon icon="angle-up"}}
            </a>
            <div>
              {{#each dns.dnsRecords as |dnsRecord dnsRecordIndex|}}
                {{#if (gt dnsRecordIndex 0)}}
                  <br />
                {{/if}}
                {{#each-in dnsRecord as |label content|}}
                  {{#if content}}
                    <div>
                      <span class="p-key">{{titleize (humanize (dasherize label))}}:
                      </span>
                      <span class="p-value">{{content}}</span>
                    </div>
                  {{/if}}
                {{/each-in}}
              {{/each}}
            </div>
          {{else}}
            <a
              href="#"
              class="expandable-title"
              {{on "click" (action "toggleExpandableTitle" "dns" "records")}}
            >
              <span class="text">{{fa-icon icon="signature" class="icon" fixedWidth=true}}
                Records</span>
              {{fa-icon icon="angle-down"}}
            </a>
          {{/if}}

        </div>
      {{/if}}
    {{/if}}
  </div>
{{/if}}

{{#if (eq block._state.activeTab "dnsWhoisHistory")}}
  <div class="dns-whois-history">
    {{#if getDnsWhoisHistoryIsRunning}}
      {{fa-icon icon="spinner-third" size="lg" fixedWidth=true spin=true}}
    {{else if gettingDnsWhoisHistoryErrorMessage}}
      <div class="alert alert-danger mt-1">
        {{gettingDnsWhoisHistoryErrorMessage}}
      </div>
    {{else if dnsWhoisHistoryFailureMessage}}
      <div class="failure-response">
        {{dnsWhoisHistoryFailureMessage}}
      </div>
    {{else if block._state.dnsWhoisHistoryHasBeenObtained}}
      {{#if (not dnsWhoisHistory.length)}}
        <span class="no-results">No Results Found</span>
      {{else}}
        {{#each dnsWhoisHistory as |_dnsWhoisHistory _dnsWhoisHistoryIndex|}}
          {{#if (gt _dnsWhoisHistoryIndex 0)}}
            <br />
          {{/if}}
          <div class="dns-whois-history-content">
            <a
              href="#"
              class="expandable-title"
              {{on
                "click"
                (action "toggleExpandableTitle" "dnsWhoisHistory" _dnsWhoisHistoryIndex)
              }}
            >
              <span class="text">{{fa-icon icon="history" class="icon" fixedWidth=true}}
                {{_dnsWhoisHistory.domainName}}
                {{#if
                  (or (gt _dnsWhoisHistory.updatedDate.length 0)
                      (gt _dnsWhoisHistory.updatedDateRaw.length 0)
                      (gt _dnsWhoisHistory.updatedDateISO8601.length 0)
                  )
                }}
                  ({{or
                    _dnsWhoisHistory.updatedDate
                    _dnsWhoisHistory.updatedDateRaw
                    _dnsWhoisHistory.updatedDateISO8601
                  }})
                {{/if}}
              </span>
              {{#if
                (get expandableTitleStates (concat "dnsWhoisHistory" _dnsWhoisHistoryIndex))
              }}
                {{fa-icon icon="angle-up"}}
              {{else}}
                {{fa-icon icon="angle-down"}}
              {{/if}}
            </a>
            {{#if
              (get expandableTitleStates (concat "dnsWhoisHistory" _dnsWhoisHistoryIndex))
            }}
              <div class="dns-whois-history-content">
              {{#if _dnsWhoisHistory.type}}
                <div>
                  <span class="p-key">Domain Type: </span>
                  <span class="p-value">{{_dnsWhoisHistory.type}}</span>
                </div>
              {{/if}}
              {{#if _dnsWhoisHistory.status}}
