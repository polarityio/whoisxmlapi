        {{#if domainAvailability.domainAvailability}}
          <div>
            <span class="p-key">Domain Purchase Availability: </span>
            <span class="p-value">{{domainAvailability.domainAvailability}}</span>
          </div>
        {{/if}}
      </div>
    {{/if}}
  </div>
{{/if}}

{{#if (eq block._state.activeTab "domainSubDomain")}}
  <div class="domain-subdomain">
    {{#if getDomainSubDomainIsRunning}}
      {{fa-icon icon="spinner-third" size="lg" fixedWidth=true spin=true}}
    {{else if gettingDomainSubDomainErrorMessage}}
      <div class="alert alert-danger mt-1">
        {{gettingDomainSubDomainErrorMessage}}
      </div>
    {{else if domainSubDomainFailureMessage}}
      <div class="failure-response">
        {{domainSubDomainFailureMessage}}
      </div>
    {{else if block._state.domainSubDomainHasBeenObtained}}
      {{#if (not displaySubDomains.length)}}
        <span class="no-results">No Results Found</span>
      {{else}}
        <div class="domain-subdomain-content">
          {{#each displaySubDomains as |subDomain|}}
            <div class="domain-subdomain">
              <button
                class="copy-btn"
                title="Copy to Clipboard"
                {{action "copyData" subDomain}}
              >
                <div id={{subDomain}}>{{fa-icon icon="copy" class="icon" fixedWidth=true}}
                  {{subDomain}}</div>
              </button>
            </div>
          {{/each}}
          {{#if (lt displaySubDomains.length domainSubDomain.length)}}
            <div class="load-more"><a
                href="#"
                {{on
                  "click"
                  (action "loadMoreDisplayItems" "displaySubDomains" "domainSubDomain")
                }}
              >
                ... Load More
              </a></div>
          {{/if}}
        </div>
      {{/if}}
    {{/if}}
  </div>
{{/if}}

{{#if (eq block._state.activeTab "reverseNs")}}
  <div class="reverse-ns">
    {{#if getReverseNsIsRunning}}
      {{fa-icon icon="spinner-third" size="lg" fixedWidth=true spin=true}}
    {{else if gettingReverseNsErrorMessage}}
      <div class="alert alert-danger mt-1">
        {{gettingReverseNsErrorMessage}}
      </div>
    {{else if reverseNsFailureMessage}}
      <div class="failure-response">
        {{reverseNsFailureMessage}}
      </div>
    {{else if block._state.reverseNsHasBeenObtained}}
      <div class="reverse-ns-content">
        {{#if (not displayReverseNs.length)}}
          <span class="no-results">No Results Found</span>
        {{else}}
          {{#each displayReverseNs as |_reverseNs _reverseNsIndex|}}
            {{#if (gt _reverseNsIndex 0)}}
              <br />
            {{/if}}
            <div class="reverse-ns-value">
              {{#if _reverseNs.name}}
                <div>
                  <span class="p-key">Name: </span>
                  <span class="p-value">{{_reverseNs.name}}</span>
                </div>
              {{/if}}
              {{#if _reverseNs.first_seen}}
                <div>
                  <span class="p-key">First Seen: </span>
                  <span class="p-value">{{moment-format
                      _reverseNs.first_seen
                      "MM/DD/YYYY HH:mm:ss"
                      timeZone=timezone
                    }}</span>
                </div>
              {{/if}}
              {{#if _reverseNs.last_visit}}
                <div>
                  <span class="p-key">Last Visit: </span>
                  <span class="p-value">{{moment-format
                      _reverseNs.last_visit
                      "MM/DD/YYYY HH:mm:ss"
                      timeZone=timezone
                    }}</span>
                </div>
              {{/if}}
            </div>
          {{/each}}
          {{#if (lt displayReverseNs.length reverseNs.length)}}
            <div class="load-more"><a
                href="#"
                {{on
                  "click"
                  (action "loadMoreDisplayItems" "displayReverseNs" "reverseNs")
                }}
              >
                ... Load More
              </a></div>
          {{/if}}
        {{/if}}
      </div>
    {{/if}}
  </div>
{{/if}}